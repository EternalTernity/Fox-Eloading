---
import "../global.css";
import "@fortawesome/fontawesome-free"

const { title } = Astro.props;
---

<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>{title}</title>
    <script src="../../node_modules/flowbite/dist/flowbite.js"></script>
  </head>
  <body class="antialiased">
      <slot />
  </body>
</html>
